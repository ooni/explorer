{
  "plugins": ["cypress"],
  "extends": [
    "next/core-web-vitals",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "@next/next/no-document-import-in-page": "off"
  },
  "ignorePatterns": ["components/vendor", "static"]
}
